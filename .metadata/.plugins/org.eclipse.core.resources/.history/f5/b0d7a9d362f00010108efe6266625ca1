spring:
 application: 
  name: apigateway-server
 cloud:
  gateway:
   routes:
     - id: category-server
       uri: http://localhost:8082
       predicates:
          - Path=/category/**

     - id: expenses-server
       uri: http://localhost:8081
       predicates:
          - Path=/expenses/** 
server: 
 port: 8080